
@{
    Layout = null;
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>新增延伸剂</title>
</head>
<body>
    <form method="post" class="pageForm required-validate"
          action="/Continue_Solid/CheckAdd" onsubmit="return validateCallback(this,dialogAjaxDone);">
        <input type="hidden" name="id" value="@ViewData["OID"]" />
        <input type="hidden" name="CreateTime" value="@ViewData["CreateTime"]" />
        <div class="pageContent">
            <div class="pageFormContent" layouth="54">
                <fieldset>
                    <legend>基本信息</legend>

                    <p class="notfirst">
                        <label>
                            检测日期：
                        </label>
                        <input name="CS_CheckTime" id="CS_CheckTime" value="@ViewData["CS_CheckTime"]"
                               class="date textInput readonly required" readonly="readonly"  type="text" size="30"
                               title="请输入检测日期" />
                    </p>
                    <p class="notfirst">
                        <label>
                            进货数量：
                        </label>
                        <input name="CS_InGoodNums" id="CS_InGoodNums" value="@ViewData["CS_InGoodNums"]" class="required digits" type="text" size="30"
                               title="请输入进货数量" />

                    </p>
                    <p class="notfirst">
                        <label>
                            抽测数量：
                        </label>
                        <input name="CS_CheckGoodNums" id="CS_CheckGoodNums" value="@ViewData["CS_CheckGoodNums"]" class="required digits" type="text" size="30"
                               title="请输入抽测数量" />

                    </p>
                    <p class="notfirst">
                        <label>
                            坩埚重：
                        </label>
                        <input name="CS_CrucibleW" id="CS_CrucibleW" value="@ViewData["CS_CrucibleW"]" class="required number" type="text" size="30"
                               title="请输入密码" />

                    </p>
                    <p class="notfirst">
                        <label>
                            拱前总重：
                        </label>
                        <input name="CS_ArchFTW1" id="CS_ArchFTW1" value="@ViewData["CS_ArchFTW1"]" class="required number" type="text" size="30"
                               title="请输入拱前总重" />

                    </p>
                    <p class="notfirst">
                        <label>
                            拱后总重：
                        </label>
                        <input name="CS_ArchFTW2" id="CS_ArchFTW2" value="@ViewData["CS_ArchFTW2"]" class="required number" type="text" size="30"
                               title="请输入拱后总重" />

                    </p>
                    <p class="notfirst">
                        <label>
                            含固量：
                        </label>
                        <input name="CS_solid" id="CS_solid" value="@ViewData["CS_solid"]" class="required number" type="text" size="30"
                               title="请输入密码" />

                    </p>
                    <p class="notfirst">
                        <label>
                            平均值：
                        </label>
                        <input name="CS_avg" id="CS_avg" value="@ViewData["CS_avg"]" class="required number" type="text" size="30"
                               title="请输入平均值" />
                    </p>
                    <p class="notfirst">
                        <label>
                            PH值：
                        </label>
                        <input name="CS_PH" id="CS_PH" value="@ViewData["CS_PH"]"  class="required number" type="text" size="30"
                               title="请输入PH值" />

                    </p>
                </fieldset>
                <fieldset>
                    <legend>厂家信息</legend>
                    <p class="notfirst">
                        <label>
                            含固量：
                        </label>
                        <input name="CS_Fsolid" id="CS_Fsolid" value="@ViewData["CS_Fsolid"]" class="required number" type="text" size="30"
                               title="请输入含固量" />
                    </p>
                    <p class="notfirst">
                        <label>
                            PH值：
                        </label>
                        <input name="CS_FPH" id="CS_FPH" value="@ViewData["CS_FPH"]"  class="required number" type="text" size="30"
                               title="请输入PH值" />
                    </p>
                    <p class="notfirst">
                        <label>
                            生产厂家：
                        </label>
                        <input name="CS_name" id="CS_name" value="@ViewData["CS_name"]"  class="required number" type="text" size="30"
                               title="请输入生产厂家" />
                    </p>
                    <p class="notfirst">
                        <label>
                            生产日期：
                        </label>
                        <input name="CS_addTime" id="CS_addTime" value="@ViewData["CS_addTime"]"
                               class="date textInput readonly required" readonly="readonly"  type="text" size="30"
                               title="请输入生产日期" />
                    </p>
                    <p class="notfirst">
                        <label>
                            合格或不合格：
                        </label>
                        <input name="CS_IsPass" id="CS_IsPass" value="@ViewData["CS_IsPass"]" class="required" type="text" size="30"
                               title="请输入密码" />
                    </p>
                    <p class="notfirst">
                        <label>
                            备注：
                        </label>
                        <input name="CS_Remark" id="CS_Remark" value="@ViewData["CS_Remark"]" class="required" type="text" size="30"
                               title="请输入密码" />
                    </p>
                    <p class="notfirst">
                        <label>
                            测试人：
                        </label>
                        <input name="CS_CheckUID" id="CS_CheckUID" value="@ViewData["CS_CheckUID"]" class="required" type="text" size="30"
                               title="请输入测试人" />
                    </p>
                    <p class="notfirst">
                        <label>
                            各月分合格率情况：
                        </label>
                        <input name="CS_IsPassMsg" id="CS_IsPassMsg" value="@ViewData["CS_IsPassMsg"]" class="required" type="text" size="30"
                               title="请输入各月分合格率情况" />
                    </p>
                  </fieldset>
                   </div>
            <div class="formBar">
                <ul>
                    <li>
                        <span id="userInfoComplete" style="color: Red; visibility: hidden; height: 30px;
                    line-height: 30px;"></span>
                    </li>
                    <li>
                        <div class="buttonActive">
                            <div class="buttonContent">
                                <button type="submit" id="btnSubmit">
                                    @(ViewData["OID"].ToString() == "0" ? "新增" : "提交")
                                </button>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="button">
                            <div class="buttonContent">
                                <button type="button" class="close">
                                    取消
                                </button>
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </form>
</body>
</html>

